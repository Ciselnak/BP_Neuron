            RowBox[{
             RowBox[{
              RowBox[{"inDataDimensions", "[", 
               RowBox[{"[", "1", "]"}], "]"}], "+", "1"}], " ", ";;", " ", 
             RowBox[{
              RowBox[{"inDataDimensions", "[", 
               RowBox[{"[", "1", "]"}], "]"}], "+", 
              RowBox[{"outDataDimensions", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}]}]}], "]"}], "]"}]}], ";", 
        "\n", " ", 
        RowBox[{"yourLearnDataIn", " ", "=", " ", 
         RowBox[{"yourLearnData", "[", 
          RowBox[{"[", 
           RowBox[{"All", ",", " ", 
            RowBox[{"1", " ", ";;", " ", 
             RowBox[{"inDataDimensions", "[", 
              RowBox[{"[", "1", "]"}], "]"}]}]}], "]"}], "]"}]}], ";", "\n", 
        " ", 
        RowBox[{"yourLearnDataOut", " ", "=", " ", 

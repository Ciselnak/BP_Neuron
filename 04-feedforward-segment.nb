               RowBox[{"yourVectorsInFold", "[", 
                RowBox[{"[", "1", "]"}], "]"}]}]}], ",", " ", 
             RowBox[{"i", "*", 
              RowBox[{"yourVectorsInFold", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}]}], "}"}]}], "]"}]}], ";", 
        "\n", " ", 
        RowBox[{"yourValidationDataIn", " ", "=", " ", 
         RowBox[{"yourValidationData", "[", 
          RowBox[{"[", 
           RowBox[{"All", ",", " ", 
            RowBox[{"1", " ", ";;", " ", 
             RowBox[{"inDataDimensions", "[", 
              RowBox[{"[", "1", "]"}], "]"}]}]}], "]"}], "]"}]}], ";", " ", 
        RowBox[{"yourValidationDataOut", " ", "=", " ", 
         RowBox[{"yourValidationData", "[", 
          RowBox[{"[", 

 RowBox[{
  RowBox[{
   RowBox[{"crossvalidate", "[", 
    RowBox[{
    "yourNet_", ",", "yourInData_", ",", "yourOutData_", ",", "yourFolds_"}], 
    "]"}], ":=", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"Off", "[", 
      StyleBox[
       RowBox[{"NeuralFit", "::", "StoppedSearch"}], "MessageName"], "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"yourVectors", " ", "=", " ", 
      RowBox[{"Take", "[", 
       RowBox[{
        RowBox[{"Dimensions", "[", "yourInData", "]"}], ",", " ", "1"}], 
       "]"}]}], ";", "\n", 
     RowBox[{"inDataDimensions", "=", 
      RowBox[{"Take", "[", 
       RowBox[{
        RowBox[{"Dimensions", "[", "yourInData", "]"}], ",", 
        RowBox[{"-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"outDataDimensions", "=", 
      RowBox[{"Take", "[", 
       RowBox[{
        RowBox[{"Dimensions", "[", "yourOutData", "]"}], ",", 
        RowBox[{"-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"yourVectorsInFold", " ", "=", " ", 
      RowBox[{"IntegerPart", "[", 
       RowBox[{"yourVectors", "/", "yourFolds"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"yourCompleteData", " ", "=", " ", 
      RowBox[{"Join", "[", 
       RowBox[{"yourInData", ",", " ", "yourOutData", ",", " ", "2"}], 
       "]"}]}], ";", "\n", 
     RowBox[{"yourCompleteData", " ", "=", " ", 
      RowBox[{"RandomSample", "[", "yourCompleteData", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"yourBest", " ", "=", " ", "1"}], ";", "\n", 
     RowBox[{"yourWorst", " ", "=", " ", "0"}], ";", "\n", 
     RowBox[{"yourSum", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"yourResults", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
       RowBox[{"i", " ", "<=", " ", "yourFolds"}], ",", " ", 
       RowBox[{"i", "++"}], ",", 
       RowBox[{"(*", 
        RowBox[{"kod", " ", "jednoho", " ", "kola", " ", "krossvalidace"}], 
        "*)"}], "\n", " ", 
       RowBox[{
        RowBox[{"yourValidationData", " ", "=", " ", 

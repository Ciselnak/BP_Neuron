        "\n", " ", 
        RowBox[{"yourSum", " ", "=", " ", 
         RowBox[{"yourSum", " ", "+", " ", "current"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"yourResults", ",", 
          RowBox[{"current", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], "]"}], ";"}]}], "\n", " ", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Print", "[", 
